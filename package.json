{
  "name": "base-react-mui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@react-keycloak/web": "^3.4.0",
    "@reduxjs/toolkit": "^1.8.2",
    "ahooks": "^3.4.0",
    "antd": "^4.20.6",
    "axios": "^0.27.2",
    "clsx": "^1.1.1",
    "dayjs": "^1.11.3",
    "env-cmd": "^10.1.0",
    "i18next": "^21.8.4",
    "i18next-browser-languagedetector": "^6.1.4",
    "i18next-http-backend": "^1.4.1",
    "keycloak-js": "^18.0.1",
    "lodash": "^4.17.21",
    "node-sass": "^7.0.1",
    "query-string": "^7.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.16.9",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.0",
    "redux-state-sync": "^3.1.2",
    "typescript": "^4.6.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "env-cmd -f .env.local react-scripts start",
    "build": "env-cmd -f .env.dev react-scripts build",
    "build-local": "env-cmd -f .env react-scripts build",
    "format": "prettier --write \"./src/**/*.{ts,tsx,json}\"",
    "test": "react-scripts test --coverage --watchAll=false --maxWorkers=4",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .ts,.tsx src --color"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@types/react-helmet": "^6.1.5",
    "@types/redux-state-sync": "^3.1.3",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-plugin-react": "^7.30.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.2",
    "prettier": "^2.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ]
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint",
      "prettier --write"
    ]
  }
}
